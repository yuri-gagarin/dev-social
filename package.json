{
  "name": "dev-social",
  "version": "1.0.0",
  "description": "social network",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "NODE_ENV=development nodemon --exec babel-node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --require @babel/register --require @babel/polyfill ",
    "debug_server": "nodemon --inspect --exec babel-node server.js",
    "seed": "NODE_ENV=test babel-node ./test/seeds/seedDB.js",
    "seed_dev": "NODE_ENV=development babel-node ./test/seeds/seedDevDB.js"
  },
  "author": "yuri-gagarin",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.6",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.2",
    "faker": "^4.1.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4"
  }
}
