{
  "name": "dev-social",
  "version": "1.0.0",
  "description": "social network",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "NODE_ENV=test nodemon --exec babel-node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --require @babel/register --require @babel/polyfill ",
    "debug_server": "nodemon --inspect --exec babel-node server.js"
  },
  "author": "yuri-gagarin",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.20",
    "multer": "^1.4.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.6.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.1",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.18.10"
  }
}
